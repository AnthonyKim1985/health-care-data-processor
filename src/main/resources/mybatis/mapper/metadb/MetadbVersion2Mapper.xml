<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bigdatacenter.dataprocessor.platform.persistence.metadb.version2.MetadbVersion2Mapper">
    <!-- RequestInfo readRequest(@Param("dataSetUID") Integer dataSetUID) -->
    <select id="readRequest"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.request.RequestInfo">
        SELECT * FROM health_care_ui.tr_dataset_list WHERE dataSetUID = #{dataSetUID};
    </select>

    <!-- List<RequestFilterInfo> readRequestFilters(@Param("dataSetUID") Integer dataSetUID) -->
    <select id="readRequestFilters"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.request.RequestFilterInfo">
        SELECT * FROM health_care_ui.tr_dataset_filter WHERE dataSetUID = #{dataSetUID};
    </select>

    <!-- List<RequestYearInfo> readRequestYears(@Param("dataSetUID") Integer dataSetUID) -->
    <select id="readRequestYears"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.request.RequestYearInfo">
        SELECT * FROM health_care_ui.tr_dataset_year WHERE dataSetUID = #{dataSetUID};
    </select>

    <!-- List<RequestIndicatorInfo> readRequestIndicators(@Param("dataSetUID") Integer dataSetUID) -->
    <select id="readRequestIndicators"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.request.RequestIndicatorInfo">
        SELECT * FROM health_care_ui.tr_dataset_indicator WHERE dataSetUID = #{dataSetUID};
    </select>

    <!-- MetaDatabaseInfo readMetaDatabase(@Param("edl_idx") Integer edlIdx) -->
    <select id="readMetaDatabase"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.meta.MetaDatabaseInfo">
        SELECT * FROM health_care_ui.chu_db_list WHERE edl_idx = #{edl_idx};
    </select>

    <!-- MetaTableInfo readMetaTable(@Param("etl_idx") Integer etlIdx) -->
    <select id="readMetaTable"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.meta.MetaTableInfo">
        SELECT * FROM health_care_ui.chu_tb_list WHERE etl_idx = #{etl_idx};
    </select>

    <!-- List<MetaColumnInfo> readMetaColumns1(@Param("ecl_idx") Integer eclIdx) -->
    <select id="readMetaColumns1" resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.meta.MetaColumnInfo">
        SELECT * FROM health_care_ui.chu_col_list WHERE ecl_idx = #{ecl_idx};
    </select>

    <!-- List<MetaColumnInfo> readMetaColumns2(@Param("edl_idx") Integer edlIdx, @Param("ecl_eng_name") String eclEngName, @Param("ecl_year") Integer eclYear) -->
    <select id="readMetaColumns2"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.meta.MetaColumnInfo">
        SELECT * FROM health_care_ui.chu_col_list WHERE edl_idx = #{edl_idx} AND ecl_eng_name = #{ecl_eng_name} AND ecl_year = #{ecl_year};
    </select>

    <!-- List<MetaRelationIndicatorWithColumn> readMetaRelationIndicatorWithColumn(@Param("eil_idx") Integer eilIdx) -->
    <select id="readMetaRelationIndicatorWithColumn"
            resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.version2.meta.MetaRelationIndicatorWithColumn">
        SELECT * FROM health_care_ui.chu_indicator_column_relation WHERE eil_idx = #{eil_idx};
    </select>

    <!-- void createFtpRequest(@Param("ftpInfo") FtpInfo ftpInfo) -->
    <insert id="createFtpRequest" parameterType="org.bigdatacenter.dataprocessor.platform.domain.metadb.common.FtpInfo">
        INSERT INTO health_care_ui.ftp_request_meta (dataSetUID, userID, ftpURI)
        VALUES (#{ftpInfo.dataSetUID}, #{ftpInfo.userID}, #{ftpInfo.ftpURI});
    </insert>

    <!-- FtpInfo readFtpRequest(@Param("dataSetUID") Integer dataSetUID) -->
    <select id="readFtpRequest" resultType="org.bigdatacenter.dataprocessor.platform.domain.metadb.common.FtpInfo">
        SELECT * FROM health_care_ui.ftp_request_meta WHERE dataSetUID = #{dataSetUID};
    </select>
</mapper>